set $mod Mod4

# Vim movement
set $left h
set $down j
set $up k
set $right l

# Your preferred terminal emulator
set $term kitty

# Variable for Doom Emacs
set $doom "emacsclient -c -a 'emacs'"

# Your preferred application launcher
set $menu "rofi -modi run,drun,window -show drun"


set $runmenu "rofi -modi run,drun,window -show run"

set $filemgr "thunar"
